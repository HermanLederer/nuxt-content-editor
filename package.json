{
  "name": "nuxt-content-writer",
  "version": "1.0.0-beta.7",
  "description": "Enable content creators to write inside your Nuxt app",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module"
  ],
  "repository": "HermanLederer/nuxt-content-writer",
  "license": "MIT",
  "author": "Herman Lederer <germans.lederers@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/module.js",
      "import": "./dist/module.es.js"
    },
    "./templates/*": "./dist/templates/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "jest",
    "release": "yarn build && npm publish"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "defu": "latest",
    "express": "^4.17.1",
    "sass": "^1.42.1",
    "upath": "latest"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@siroc/jest-preset": "latest",
    "@types/jest": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "eslint": "latest",
    "jest": "latest",
    "nuxt-edge": "latest",
    "playwright": "latest",
    "siroc": "latest",
    "standard-version": "latest"
  }
}
